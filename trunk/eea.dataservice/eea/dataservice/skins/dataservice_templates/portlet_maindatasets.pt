<html xmlns:tal="http://xml.zope.org/namespaces/tal"
      xmlns:metal="http://xml.zope.org/namespaces/metal"
      i18n:domain="plone">
<body>

<div metal:define-macro="portlet"
       i18n:domain="plone"
       tal:omit-tag=""
       tal:define="view context/@@getMainDatasets">

<dl class="portlet portlet-rss"
    style="float: right; clear: right; margin-right: 0pt; width: 17em; padding: 7px;">

    <dt class="portletHeader" i18n:translate="">Main data sets</dt>
    <dd class="portletItem" tal:repeat="dataset view">
        <a tal:content="dataset/Title" tal:attributes="href dataset/absolute_url"/>
    </dd>
    <dd class="portletFooter"></dd>

</dl>
<div class="visualClear"><!-- &nbsp;--></div>
</div>
</body>
</html>
