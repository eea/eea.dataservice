<html xmlns="http://www.w3.org/1999/xhtml" xml:lang="en"
      lang="en"
      metal:use-macro="here/main_template/macros/master"
      i18n:domain="plone">
<body>

<div metal:fill-slot="main">
 <h1 i18n:translate="">Convert all figures</h1>

 <form name="frmConvert" action="@@convertAllMaps" method="put">
  <input class="context" type="submit" name="convert" value="Convert" />
  <input class="context" type="submit" name="cancel" value="Cancel" />
 </form>

 <ul>
  <li tal:repeat="ob python:context.objectValues('EEAFigureFile')"
      tal:content="ob/title_or_id" />
 </ul>

</div>

</body>
</html>
